{
  "name": "powermd",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "ng build --prod --base-href 'https://powermd.phodal.com' && echo 'powermd.phodal.com' > dist/CNAME && npx angular-cli-ghpages"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.3",
    "@angular/cdk": "~9.1.0",
    "@angular/common": "~9.0.3",
    "@angular/compiler": "~9.0.3",
    "@angular/core": "~9.0.3",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "~9.0.3",
    "@angular/material": "^9.1.0",
    "@angular/platform-browser": "~9.0.3",
    "@angular/platform-browser-dynamic": "~9.0.3",
    "@angular/router": "~9.0.3",
    "@ecodev/fab-speed-dial": "^5.0.3",
    "@swimlane/ngx-datatable": "^16.0.3",
    "angular-calendar": "^0.27.14",
    "angular-split": "^3.0.3",
    "angular-vertical-timeline": "^0.6.0",
    "angularx-flatpickr": "^6.1.0",
    "core-js": "^2.5.4",
    "d3": "3.5.17",
    "date-fns": "^1.30.1",
    "flatpickr": "^4.6.2",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.15",
    "material-design-icons": "^3.0.1",
    "mousetrap": "^1.6.3",
    "ng2-dragula": "^2.1.1",
    "ngx-contextmenu": "^5.4.0",
    "ngx-markdown": "^9.0.0",
    "normalize.css": "^8.0.1",
    "rxjs": "~6.5.4",
    "shortid": "^2.2.14",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.4",
    "@angular/cli": "~9.0.3",
    "@angular/compiler-cli": "~9.0.3",
    "@angular/language-service": "~9.0.3",
    "@types/d3": "^3.5.17",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.136",
    "@types/medium-editor": "^5.0.3",
    "@types/mousetrap": "^1.6.3",
    "@types/shortid": "^0.0.29",
    "angular-cli-ghpages": "^0.5.3",
    "husky": "^1.3.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "stylelint": "^9.10.1",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ng lint",
      "pre-push": "ng test && ng build --prod --aot"
    }
  },
  "lint-staged": {
    "src/app/*.{css,scss}": [
      "stylelint --syntax=scss",
      "prettier --parser --write",
      "git add"
    ],
    "{src,test}/**/*.ts": [
      "prettier --write --single-quote",
      "git add"
    ]
  }
}
